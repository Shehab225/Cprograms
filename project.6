#include<stdio.h>

int integer(int);
int character(int);
int doubl(int );

int main(void){
    int num=0;
    int i;
    int sum=0;
    char c;
    int n;
    scanf("%d",&num);
    for(i=0;i<num;i++){
        scanf("%d %c",&n,&c);
        if (c == 'i'){
            sum = sum+integer(n);
        }else if(c == 'c'){
            sum = sum+character(n);
        }else if(c == 'd'){
            sum = sum + doubl(n);
        }else{
            printf("Invalid tracking code type ");
            return 0;
        }
    }
    printf("%d bytes",sum);
} 
     int integer(int n){
         
         int result;
        result= sizeof(int)*n;
        return result;
     }
     
     
     int character(int n ){
         int result;
         result = sizeof(char)*n;
         return result;
     }
         
    int doubl(int n){
        int result;
        result = sizeof(double)*n;
        return result;
    }     
         
         
    




